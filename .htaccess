Options +FollowSymlinks
RewriteEngine On
RewriteBase /
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

#RewriteRule ^$ index.php?section=docs&content=index [L]
#RewriteRule ^/?$ index.php?section=docs&content=index [L]
#RewriteRule ^/index\.php?$ index.php?section=docs&content=index [L]

RewriteRule ^([^/]+)/?$ %2index.php?section=$1&content=index [L]
RewriteRule ^([^/]+)/([^/]+)\/?$ index.php?section=$1&content=$2 [L]
